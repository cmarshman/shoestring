{
  "name": "rthauthserver",
  "version": "1.0.0",
  "description": "Sample security protocol",
  "main": "server.js",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "install": "cd client && npm install",
    "build": "cd client && npm run build",
    "heroku-postbuild": "npm run build"
  },
  "author": "Strategic Machines",
  "license": "MIT",
  "dependencies": {
    "@mdi/js": "^5.3.45",
    "@mdi/react": "^1.4.0",
    "ajax": "0.0.4",
    "axios": "^0.19.2",
    "bcrypt": "^5.0.0",
    "bulma-graphs": "^0.2.1",
    "dot-prop": "^5.2.0",
    "dotenv": "^6.1.0",
    "email-validator": "^2.0.4",
    "express": "^4.17.1",
    "formik": "^2.1.4",
    "if-env": "^1.0.4",
    "jquery": "^3.5.1",
    "jsonwebtoken": "^8.1.0",
    "logkitty": "^0.7.1",
    "mdi-react": "^7.3.0",
    "modal": "^1.2.0",
    "moment": "^2.24.0",
    "mongoose": "^5.9.7",
    "morgan": "^1.9.1",
    "plaid": "^4.10.0",
    "rc-scroll-anim": "^2.7.4",
    "rc-tween-one": "^2.7.2",
    "react-bootstrap": "^1.0.0",
    "react-helmet": "^6.1.0",
    "react-modal": "^3.11.2",
    "react-native": "^0.62.2",
    "react-plaid-link": "^2.1.0",
    "styled-components": "^5.1.1",
    "yarn": "^1.22.4",
    "yup": "^0.28.3"
  },
  "devDependencies": {
    "concurrently": "^4.1.0",
    "nodemon": "^1.18.7"
  }
}
